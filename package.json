{
  "name": "d2l-course-image",
  "description": "Brightspace Course Image",
  "private": true,
  "scripts": {
    "postinstall": "polymer install --variants",
    "lint": "npm run lint:html && npm run lint:wc",
    "lint:html": "eslint *.html test/*.html test/*.js",
    "lint:wc": "polymer lint",
    "test:polymer:local": "polymer test --skip-plugin sauce",
    "test:polymer:sauce": "polymer test --skip-plugin local",
    "test": "npm run lint && npm run test:polymer:local"
  },
  "homepage": "https://github.com/Brightspace/course-image",
  "repository": {
    "type": "git",
    "url": "https://github.com/Brightspace/course-image.git"
  },
  "bugs": {
    "url": "https://github.com/Brightspace/course-image/issues"
  },
  "keywords": [
    "d2l",
    "course",
    "image"
  ],
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "devDependencies": {
    "eslint": "^4.15.0",
    "eslint-config-brightspace": "^0.4.0",
    "eslint-plugin-html": "^4.0.1",
    "polymer-cli": "^1.5.7"
  }
}
